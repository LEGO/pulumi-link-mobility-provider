{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-irregular-whitespace": 1,
      "no-useless-escape": 0,
      "@typescript-eslint/no-unused-vars": "error"
    }
  }